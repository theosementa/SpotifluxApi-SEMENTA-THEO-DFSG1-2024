{% extends 'base.html.twig' %}

{% block title %}ArtistEntity{% endblock %}

{% block body %}
<div class="container">
    <a href="{{ path('app_web_artist_entity_index') }}" class="btn btn-primary" >Retour aux artistes</a>

    <table class="table table-bordered mt-3">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ artist_entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ artist_entity.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_web_artist_entity_edit', {'id': artist_entity.id}) }}" class="btn btn-primary mb-2">Modifier</a>

    {{ include('web/artist_entity/_delete_form.html.twig') }}
</div>
{% endblock %}
